@using System.Linq

@model ClientDetails
@{
    ViewBag.Nav = Navigation.Home;
}

@section Title {
    Client Details
}

<h2>Client Details</h2>



<div class="col-sm-10">
    <p>
        Browser: @Model.browser
    </p>
    <p>
        Client OS: @Model.os
    </p>
    <p>
        User Host Address: @Model.userHostAddress
    </p>

    
</div>

